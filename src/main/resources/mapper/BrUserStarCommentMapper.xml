<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tyf.bookreaderplus.book.dao.BrUserStarCommentMapper">
    <insert id="replace">
        replace into br_user_star_comment
        (
            comment_id,
            user_id,
            status
        )
        values
        (
            #{commentId},
            #{userId},
            #{status}
        )
    </insert>
</mapper>
